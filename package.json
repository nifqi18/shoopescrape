{
  "name": "react-express-app",
  "version": "0.1.0",
  "description": "Awesome and super-simple app with React front-end and Express back-end.",
  "license": "MIT",
  "private": false,
  "repository": {
    "url": "https://github.com/alexdevero/react-express-app",
    "type": "git"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "keywords": [
    "app",
    "boilerplate",
    "express",
    "react",
    "starter"
  ],
  "bugs": {
    "url": "https://github.com/alexdevero/react-express-app/issues"
  },
  "proxy": "http://localhost:3001",
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "start": "concurrently \"yarn start-frontend || npm run start-frontend\" \"yarn start-backend || npm run start-backend\"",
    "start-backend": "nodemon server/server.js",
    "start-frontend": "react-scripts start",
    "build": "node ./buildScript",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.4",
    "debug": "~4.1.1",
    "express": "^4.17.1",
    "fs-extra": "8.1.0",
    "morgan": "^1.9.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "^4.3.1",
    "superagent": "^6.1.0"
  },
  "devDependencies": {
    "bulma": "^0.9.1",
    "concurrently": "^4.1.1",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "nodemon": "^1.19.1",
    "react-scripts": "^3.0.1",
    "typescript": "^3.6.4"
  }
}
